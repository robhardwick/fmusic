state = 0

function f(t)
	if (t % 100) == 0 then
		status = 144
		data2 = 90
		state = state + 1
		if state >= 127 then
			state = 0
		end
		if (t % 200) == 0 then
			data1 = state + 13
		else
			data1 = state - 13
		end
		if data1 > 127 then
			data1 = data1 - 127
		end
		if data1 < 0 then
			data1 = 127 + data1
		end
		return status, data1, data2
	end
	return 0, 0, 0
end
